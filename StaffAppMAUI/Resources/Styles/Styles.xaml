    <Style x:Key="cardButtonStyle" TargetType="dxcore:DXButton">
        <Setter Property="CornerRadius" Value="20" />
        <Setter Property="BorderThickness" Value="0" />
        <Setter Property="TextColor" Value="{StaticResource TextThemeColor}" />
        <Setter Property="BackgroundColor" Value="{StaticResource BackgroundThemeColor}" />
        <Setter Property="BorderThickness" Value="0" />
        <Setter Property="Margin" Value="10" />
        <Setter Property="Padding" Value="18" />
        <Setter Property="Shadow">
            <Setter.Value>
                <OnPlatform x:TypeArguments="Shadow">
                    <On Platform="iOS">
                        <On.Value>
                            <Shadow Brush="{StaticResource ShadowBrushiOS}" Radius="8" />
                        </On.Value>
                    </On>
                    <On Platform="Android">
                        <On.Value>
                            <Shadow Brush="{StaticResource ShadowBrushAndroid}" Radius="8" />
                        </On.Value>
                    </On>
                </OnPlatform>
            </Setter.Value>
        </Setter>
    </Style>

